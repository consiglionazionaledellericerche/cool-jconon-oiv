<?xml version="1.0" encoding="UTF-8"?>

<model name="fpperson:model" xmlns="http://www.alfresco.org/model/dictionary/1.0">

   <description>CNR Person Model</description>
   <author>Francesco Uliana</author>
   <version>1.0</version>

   <imports>
      <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
      <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
   </imports>

   <namespaces>
      <namespace uri="http://performance.gov.it/model/fpperson" prefix="fpperson"/>
   </namespaces>

   <aspects>
      <aspect name="fpperson:metadati">
         <title>Dati anagrafici DFP</title>
         <properties>
            <property name="fpperson:amministrazione">
               <title>Amministrazione</title>
               <type>d:text</type>
               <multiple>true</multiple>
               <index enabled="true">
                 <atomic>true</atomic>
                 <stored>true</stored>
                 <tokenised>false</tokenised>
               </index>
            </property>
         </properties>
      </aspect>
   </aspects>
</model>
